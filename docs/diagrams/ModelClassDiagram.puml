@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model <<Rectangle>>{
Class "<<Interface>>\nReadOnlyAddressBook"
Class "<<Interface>>\nReadOnlyUserPrefs"
Class "<<Interface>>\nModel"
Class AddressBook
Class "<<Interface>>\nReadOnlyAddressBook"
Class "<<Interface>>\nModel"
Class ModelManager
Class UserPrefs
Class "<<Interface>>\nReadOnlyUserPrefs"


Class UniquePersonList
Class Person
Class Address
Class Email
Class Name
Class Phone
Class Tag

}

Class HiddenOutside #FFFFFF
HiddenOutside ..> "<<Interface>>\nModel"

AddressBook .up.|> "<<Interface>>\nReadOnlyAddressBook"

ModelManager .up.|> "<<Interface>>\nModel"
"<<Interface>>\nModel" .right.> "<<Interface>>\nReadOnlyUserPrefs"
"<<Interface>>\nModel" .left.> "<<Interface>>\nReadOnlyAddressBook"
ModelManager -left-> "1" AddressBook
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> "<<Interface>>\nReadOnlyUserPrefs"

AddressBook *--> "1" UniquePersonList
UniquePersonList --> "~* all" Person
Person *--> Name
Person *--> Phone
Person *--> Email
Person *--> Address
Person *--> "*" Tag

Name -[hidden]right-> Phone
Phone -[hidden]right-> Address
Address -[hidden]right-> Email

ModelManager -->"~* filtered" Person
@enduml
