@startuml

Abstract Class Command {
---
+ {abstract} execute(Model) : CommandResult
{exception : CommandException}
}

Class RemarkCommand {
+ COMMAND_WORD : String
+ MESSAGE_USAGE : String
+ MESSAGE_NOT_IMPLEMENTED_YET : String
---
+ execute(Model) : CommandResult
{exception : CommandException}
}

RemarkCommand -up-|> Command

@enduml

